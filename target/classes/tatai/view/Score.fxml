<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane stylesheets="@Design.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tatai.view.ScoreController">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" spacing="20.0" AnchorPane.bottomAnchor="70.0" AnchorPane.leftAnchor="25.0" AnchorPane.rightAnchor="25.0" AnchorPane.topAnchor="30.0">
         <children>
            <Label id="score-label" text="Congratulations!" />
            <Label id="score-label" text="You scored" />
            <Label id="score-label" fx:id="scoreLbl" layoutX="10.0" layoutY="27.0" text="out of 10" />
            <Button fx:id="tryagainBtn" mnemonicParsing="false" onAction="#tryAgainBtn" text="Try Again" />
            <Button fx:id="nextlevelBtn" mnemonicParsing="false" onAction="#nextLevelBtn" text="Next Level" />
         </children>
      </VBox>
      <Button id="small-button" layoutX="24.0" layoutY="24.0" mnemonicParsing="false" prefHeight="39.0" prefWidth="100" text="Help" AnchorPane.bottomAnchor="15.0" AnchorPane.leftAnchor="15.0">
         <font>
            <Font name="Berlin Sans FB" size="14.0" />
         </font>
      </Button>
      <Button id="small-button" fx:id="menu" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#menuBtn" prefHeight="39.0" prefWidth="100" text="Home">
         <font>
            <Font name="Berlin Sans FB" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
