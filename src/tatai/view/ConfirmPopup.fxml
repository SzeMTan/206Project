<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="popup-root" stylesheets="@Design.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tatai.view.ConfirmPopupController">
   <children>
      <ImageView fitHeight="76.0" fitWidth="117.0" layoutX="99.0" layoutY="124.0" AnchorPane.bottomAnchor="0.0">
         <image>
            <Image url="@../../main/resources/tahiquit.png" />
         </image>
      </ImageView>
      <Label fx:id="_question" alignment="CENTER" layoutY="42.0" prefHeight="17.0" prefWidth="300.0" text="Are you sure you want to leave? " textAlignment="CENTER">
         <font>
            <Font name="Berlin Sans FB" size="14.0" />
         </font>
      </Label>
      <Label fx:id="_consequence" alignment="CENTER" layoutY="83.0" prefHeight="16.0" prefWidth="300.0" text="(Your progress and score will be deleted)">
         <font>
            <Font name="Berlin Sans FB" size="14.0" />
         </font>
      </Label>
      <Button id="really-small-button" fx:id="_yesBtn" layoutX="29.0" layoutY="132.0" mnemonicParsing="false" text="Yes" AnchorPane.bottomAnchor="38.0" AnchorPane.leftAnchor="29.0" AnchorPane.rightAnchor="191.0" />
      <Button id="really-small-button" fx:id="_noBtn" layoutX="179.0" layoutY="132.0" mnemonicParsing="false" text="No" AnchorPane.bottomAnchor="38.0" AnchorPane.rightAnchor="29.0" />
   </children>
</AnchorPane>
