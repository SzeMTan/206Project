<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>

<AnchorPane stylesheets="@popupDesign.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tatai.view.BackConfirmationController">
   <children>
      <ImageView fitHeight="92.0" fitWidth="133.0" layoutX="219.0" layoutY="108.0">
         <image>
            <Image url="@../../main/resources/tahiquit.png" />
         </image>
      </ImageView>
      <Label layoutX="66.0" layoutY="29.0" prefHeight="17.0" prefWidth="219.0" text="Are you sure you want to go back?" />
      <Label layoutX="88.0" layoutY="60.0" text="(All progress will be deleted)" />
      <HBox layoutX="75.0" layoutY="87.0" prefHeight="100.0" prefWidth="200.0">
         <children>
            <StackPane prefHeight="150.0" prefWidth="200.0">
               <children>
                  <Button fx:id="_yesBtn" mnemonicParsing="false" text="Yes" />
               </children>
            </StackPane>
            <StackPane prefHeight="150.0" prefWidth="200.0">
               <children>
                  <Button fx:id="_noBtn" mnemonicParsing="false" text="no" />
               </children>
            </StackPane>
         </children>
      </HBox>
   </children>
</AnchorPane>
