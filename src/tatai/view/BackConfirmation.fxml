<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>

<AnchorPane stylesheets="@popupDesign.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tatai.view.BackConfirmationController">
   <children>
      <Label layoutX="33.0" layoutY="29.0" text="Are you sure you want to go back?" />
      <Label layoutX="50.0" layoutY="57.0" text="(All progress will be deleted)" />
      <HBox layoutX="26.0" layoutY="86.0" prefHeight="100.0" prefWidth="200.0">
         <children>
            <StackPane prefHeight="150.0" prefWidth="200.0">
               <children>
                  <Button fx:id="_yesBtn" mnemonicParsing="false" text="Yes" />
               </children>
            </StackPane>
            <StackPane prefHeight="150.0" prefWidth="200.0">
               <children>
                  <Button fx:id="_noBtn" mnemonicParsing="false" text="no" />
               </children>
            </StackPane>
         </children>
      </HBox>
   </children>
</AnchorPane>
