<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.InnerShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane stylesheets="@Design.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tatai.view.ScoreController">
   <children>
      <Label alignment="CENTER" layoutX="147.0" layoutY="41.0" prefHeight="77.0" prefWidth="356.0" text="Congratulations!  ">
         <font>
            <Font name="Berlin Sans FB" size="38.0" />
         </font>
      </Label>
      <Label fx:id="scorelabel" alignment="CENTER" layoutX="181.0" layoutY="184.0" prefHeight="77.0" prefWidth="290.0" text="9 out of 10">
         <font>
            <Font name="Berlin Sans FB" size="45.0" />
         </font>
      </Label>
      <Button fx:id="tryagainbtn" layoutX="181.0" layoutY="291.0" mnemonicParsing="false" onAction="#tryAgainBtn" prefHeight="39.0" prefWidth="149.0" text="Try Again">
         <font>
            <Font name="Berlin Sans FB" size="20.0" />
         </font>
         <effect>
            <InnerShadow />
         </effect>
      </Button>
      <Button id="small-button" fx:id="menu" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#menuBtn" prefHeight="39.0" prefWidth="100" text="Home">
         <font>
            <Font name="Berlin Sans FB" size="14.0" />
         </font>
         <effect>
            <InnerShadow />
         </effect>
      </Button>
      <Button fx:id="nextlevel" layoutX="346.0" layoutY="291.0" mnemonicParsing="false" onAction="#nextLevelBtn" prefHeight="39.0" prefWidth="149.0" text="Next Level">
         <font>
            <Font name="Berlin Sans FB" size="20.0" />
         </font>
         <effect>
            <InnerShadow />
         </effect>
      </Button>
      <Label alignment="CENTER" layoutX="166.0" layoutY="107.0" prefHeight="77.0" prefWidth="319.0" text="You Scored:">
         <font>
            <Font name="Berlin Sans FB" size="30.0" />
         </font>
      </Label>
      <Button id="small-button" fx:id="menu1" layoutX="14.0" layoutY="444.0" mnemonicParsing="false" prefHeight="39.0" prefWidth="100" text="Help">
         <font>
            <Font name="Berlin Sans FB" size="14.0" />
         </font>
         <effect>
            <InnerShadow />
         </effect>
      </Button>
   </children>
</AnchorPane>
