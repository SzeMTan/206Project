<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.InnerShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tatai.view.ScoreController">
   <children>
      <Pane prefHeight="400.0" prefWidth="500.0" style="-fx-background-color: #84e1e0;">
         <children>
            <AnchorPane prefHeight="400.0" prefWidth="500.0">
               <children>
                  <Label alignment="CENTER" layoutX="72.0" layoutY="-2.0" prefHeight="77.0" prefWidth="356.0" text="Congratulations!  ">
                     <font>
                        <Font name="Berlin Sans FB" size="38.0" />
                     </font>
                  </Label>
                  <Label fx:id="scorelabel" alignment="CENTER" layoutX="105.0" layoutY="114.0" prefHeight="77.0" prefWidth="290.0" text="9 out of 10">
                     <font>
                        <Font name="Berlin Sans FB" size="45.0" />
                     </font>
                  </Label>
                  <Button fx:id="tryagainbtn" layoutX="171.0" layoutY="249.0" mnemonicParsing="false" onAction="#tryAgainBtn" prefHeight="39.0" prefWidth="149.0" text="Try Again">
                     <font>
                        <Font name="Berlin Sans FB" size="20.0" />
                     </font>
                     <effect>
                        <InnerShadow />
                     </effect>
                  </Button>
                  <Button fx:id="menu" layoutX="14.0" layoutY="347.0" mnemonicParsing="false" onAction="#menuBtn" prefHeight="39.0" prefWidth="100" text="Main Menu" AnchorPane.bottomAnchor="14.0" AnchorPane.leftAnchor="14.0">
                     <font>
                        <Font name="Berlin Sans FB" size="14.0" />
                     </font>
                     <effect>
                        <InnerShadow />
                     </effect>
                  </Button>
                  <Button fx:id="nextlevel" layoutX="171.0" layoutY="200.0" mnemonicParsing="false" onAction="#nextLevelBtn" prefHeight="39.0" prefWidth="149.0" text="Next Level">
                     <font>
                        <Font name="Berlin Sans FB" size="20.0" />
                     </font>
                     <effect>
                        <InnerShadow />
                     </effect>
                  </Button>
                  <Label alignment="CENTER" layoutX="91.0" layoutY="57.0" prefHeight="77.0" prefWidth="319.0" text="You Scored:">
                     <font>
                        <Font name="Berlin Sans FB" size="30.0" />
                     </font>
                  </Label>
               </children>
            </AnchorPane>
         </children>
      </Pane>
   </children>
</AnchorPane>
